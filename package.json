{
  "name": "wolkenkit-boards",
  "version": "1.0.0",
  "description": "wolkenkit-boards is a tool for collaboratively organizing notes.",
  "contributors": [
    {
      "name": "Golo Roden",
      "email": "golo.roden@thenativeweb.io"
    },
    {
      "name": "Matthias Wagler",
      "email": "matthias.wagler@thenativeweb.io"
    },
    {
      "name": "Ren√© Viering",
      "email": "rene.viering@gmail.com"
    }
  ],
  "wolkenkit": {
    "application": "boards",
    "runtime": {
      "version": "latest"
    },
    "environments": {
      "default": {
        "api": {
          "port": 3000,
          "allowAccessFrom": "*"
        },
        "fileStorage": {
          "allowAccessFrom": "*"
        },
        "node": {
          "environment": "development"
        },
        "identityProviders": [
          {
            "issuer": "https://thenativeweb.eu.auth0.com/",
            "certificate": "/server/keys/auth0"
          }
        ]
      }
    }
  },
  "scripts": {
    "postinstall": "cd client && npm install"
  },
  "dependencies": {
    "slugify": "1.3.4",
    "uuidv4": "2.0.0",
    "wolkenkit-command-tools": "3.0.0"
  },
  "devDependencies": {
    "roboter": "3.0.3"
  },
  "license": "AGPL-3.0"
}
